d_settings&quot;: [&#x000A;        {&#x000A;          &quot;custom_field_context_type&quot;: &quot;ClientRegistrationSettings&quot;,&#x000A;          &quot;client_managers&quot;: &quot;required&quot;,&#x000A;          &quot;non_client_managers&quot;: &quot;required&quot;,&#x000A;          &quot;dependents&quot;: &quot;required&quot;&#x000A;        },&#x000A;        {&#x000A;          &quot;custom_field_context_type&quot;: &quot;ClientProfileSettings&quot;,&#x000A;          &quot;visibility&quot;: &quot;everyone&quot;&#x000A;        }&#x000A;      ]&#x000A;    },&#x000A;    {&#x000A;      &quot;id&quot;: 7,&#x000A;      &quot;name&quot;: &quot;Are you a robot?&quot;,&#x000A;      &quot;why&quot;: &quot;Because Cylons&quot;,&#x000A;      &quot;data_type&quot;: &quot;boolean&quot;,&#x000A;      &quot;custom_field_settings&quot;: [&#x000A;        {&#x000A;          &quot;custom_field_context_type&quot;: &quot;ClientRegistrationSettings&quot;,&#x000A;          &quot;client_managers&quot;: &quot;required&quot;,&#x000A;          &quot;non_client_managers&quot;: &quot;required&quot;,&#x000A;          &quot;dependents&quot;: &quot;optional&quot;&#x000A;        },&#x000A;        {&#x000A;          &quot;custom_field_context_type&quot;: &quot;ClientProfileSettings&quot;,&#x000A;          &quot;visibility&quot;: &quot;staff_only&quot;&#x000A;        }&#x000A;      ]&#x000A;    }&#x000A;  ],&#x000A;  &quot;total_count&quot;: 3&#x000A;}&#x000A;</pre></p></code></div>
